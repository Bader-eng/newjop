<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <a href="/">Home</a>
    <a href="/sea">search</a>
    <% data.forEach(element => { %>
     <div>
         <li>
             <%= element.title %> 
         </li>
         <li>
            <%= element.company %> 
        </li>
        <li>
            <%= element.location %> 
        </li>
        <li>
            <%= element.url %> 
        </li>
        <a href="/details/<%= element.id %> ">details</a>
     </div>
    <% }) %>
</body>
</html>